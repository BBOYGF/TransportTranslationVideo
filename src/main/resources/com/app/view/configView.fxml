<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.view.ConfigView">
    <columnConstraints>
        <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" minWidth="-Infinity" />
        <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
        <ColumnConstraints maxWidth="1.7976931348623157E308" minWidth="-Infinity" />

    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints vgrow="SOMETIMES" />
        <RowConstraints />
        <RowConstraints maxHeight="1.7976931348623157E308" vgrow="SOMETIMES" />
        <RowConstraints />
        <RowConstraints maxHeight="1.7976931348623157E308" vgrow="ALWAYS" />
        <RowConstraints maxHeight="1.7976931348623157E308" vgrow="SOMETIMES" />
    </rowConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <children>
        <Label alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="URL：" />
        <TextField fx:id="urlTextField" GridPane.columnIndex="1" />
        <Label alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="标题：" GridPane.rowIndex="1" />
        <TextArea fx:id="titleTextField" maxHeight="1.7976931348623157E308" prefHeight="100.0" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="开头短视频：" GridPane.rowIndex="2" />
        <TextField fx:id="beginTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <Button fx:id="choiceBeginButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" onAction="#onChoiceBeginButtonClick" text="选择视频文件" GridPane.columnIndex="2" GridPane.rowIndex="2" />
        <Button fx:id="uploadButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onUploadButtonClick" prefHeight="50.0" text="一键搬运" GridPane.columnIndex="1" GridPane.rowIndex="4" />
        <TextArea fx:id="logTextArea" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" wrapText="true" GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="5" GridPane.vgrow="ALWAYS" />
    </children>
</GridPane>
